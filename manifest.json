{
    "name": "Password Manager",
    "version": "1.0.0",
    "manifest_version": 3,
    "author": "Leon, Johannes",
    "permissions": ["activeTab", "storage"],
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
      ],
    
    "action":{
        "default_popup": "extPage/index.html",
        "default_title": "Password Manager"
    }
}
