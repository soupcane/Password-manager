{
    "name": "Password Manager",
    "version": "1.0.0",
    "manifest_version": 3,
    "author": "Leon Svensson, Johannes Wik",
    "permissions": ["activeTab", "storage", "downloads"],
    "background": {
      "service_worker": "service_worker.js",
      "type": "module"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js", "FileSaver.js"]
        }
      ],
    
    "action":{
        "default_popup": "index.html",
        "default_title": "Password Manager"
    }
}
